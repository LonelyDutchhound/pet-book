{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    // override tslint-react rules here
    "jsx-wrap-multiline": false,
    "no-submodule-imports": false,
    "max-line-length": [
      true,
      {
        "limit": 100,
        "check-strings": true,
        "check-regex": true
      }
    ]
  }
}